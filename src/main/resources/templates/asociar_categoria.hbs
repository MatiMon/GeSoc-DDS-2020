<!DOCTYPE html>
<script>
    window.addEventListener("DOMContentLoaded", function () {
        let form = document.getElementById("formLogout");
        document.getElementById("u39").addEventListener("click", function () {
            form.submit();
        });
    });


</script>
<html>
<head>
    <title>Asociar categoria</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <link href="/css/axure_rp_page.css" type="text/css" rel="stylesheet"/>
    <link href="/css/styles.css" type="text/css" rel="stylesheet"/>
    <link href="/css/stylesGestionEntidades.css" type="text/css" rel="stylesheet"/>
</head>
<body>
<div id="base" class="">

    <!-- Header (Rectangle) -->
    <div id="u35" class="ax_default box_2" data-label="Header">
        <div id="u35_div" class=""></div>
        <div id="u35_text" class="text " style="display:none; visibility: hidden">
            <p></p>
        </div>
    </div>

    <!-- Nombre organizacion (Rectangle) -->
    <div id="u36" class="ax_default heading_1" data-label="Nombre organizacion">
        <div id="u36_div" class=""></div>
        <div id="u36_text" class="text ">
            <p><span style="margin-left: -200px;">{{organizacion}}</span></p>
        </div>
    </div>

    <!-- Nombre usuario (Rectangle) -->
    <div id="u37" class="ax_default heading_2" data-label="Nombre usuario">
        <div id="u37_div" class=""></div>
        <div id="u37_text" class="text ">
            <p><span>{{nombreUsuario}}</span></p>
        </div>
    </div>

    <!-- Logo (Image) -->
    <div id="u38" class="ax_default image" data-label="Logo">
        <img id="u38_img" class="img " src="/img/logo_u11.png"/>
        <div id="u38_text" class="text " style="display:none; visibility: hidden">
            <p></p>
        </div>
    </div>

    <!-- Boton log out (Rectangle) -->
    <form action="/" method="get" id="formLogout">
        <div id="u39" class="ax_default button" data-label="Boton log out" style="cursor: pointer;">
            <div id="u39_div" class=""></div>
            <div id="u39_text" class="text ">
                <p><span>Log out</span></p>
            </div>
        </div>
    </form>

    <!-- Panel derecho arriba (Rectangle) -->
    <div id="u40" class="ax_default box_2" data-label="Panel derecho arriba">
        <div id="u40_div" class=""></div>
        <div id="u40_text" class="text ">
            <p></p>
        </div>
    </div>


    <!-- Panel izquierdo (Rectangle) -->
    <div id="u43" class="ax_default box_2" data-label="Panel izquierdo">
        <div id="u43_div" class=""></div>
        <div id="u43_text" class="text " style="display:none; visibility: hidden">
            <p></p>
        </div>
    </div>

    <!-- Boton Gestionar Entidades (Rectangle) -->
    <form action="/entidades" method="get">
        <button id="u44" class="ax_default button" data-label="Boton Gestionar Entidades">
            <div id="u44_div" class=""></div>
            <div id="u44_text" class="text ">
                <p><span>Gestionar Entidades</span></p>
            </div>
        </button>
    </form>

    <!-- Boton Gestionar Egresos (Rectangle) -->
    <form action="/egresos" method="get">
        <button id="u45" class="ax_default button" data-label="Boton Gestionar Egresos">
            <div id="u45_div" class=""></div>
            <div id="u45_text" class="text ">
                <p><span>Gestionar Egresos</span></p>
            </div>
        </button>
    </form>

    <!-- Boton Inicio (Rectangle) -->
    <form action="/principal" method="get">
        <button href="/principal" id="u46" class="ax_default button" data-label="Boton Inicio">
            <div id="u46_div" class=""></div>
            <div id="u46_text" class="text ">
                <p><span>Inicio</span></p>
            </div>
        </button>
    </form>

        <!-- Droplist categorias (Droplist) -->
        <div id="u52" class="ax_default droplist" data-label="Droplist categorias">
            <div id="u52_div" class=""></div>
            <select id="u52_input" class="u52_input">
                <option value="Categoria">Categoria</option>
                {{#each categorias}}
                    <option value="{{id}}" >{{nombre}}</option>
                {{/each}}
            </select>
        </div>

        <!-- Boton Asociar (Shape) -->
        <form action="/entidades" method="get" >
            <div id="u53" class="ax_default button"  style="cursor: pointer;">
                <img id="u53_img" class="img " src="/img/boton_asociar_u53.svg"/>
                <div id="u53_div" class=""></div>
                <div id="u53_text" class="text ">
                    <p><span>Asociar</span></p>
                </div>
            </div>
        </form>

        <!-- Texto seleccionar categoria) -->
        <div id="u54" class="ax_default paragraph">
            <div id="u54_div" class=""></div>
            <div id="u54_text" class="text ">
                <p><span>Seleccione una categoria:</span></p>
            </div>
        </div>


</div>
</body>
</html>
